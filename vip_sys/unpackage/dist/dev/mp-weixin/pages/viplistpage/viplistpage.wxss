.content {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  background-color: #dadada;
}
.content .page-title {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.content .page-title .title {
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  font-size: 36rpx;
  border-bottom: #000000 1rpx solid;
}
.content .order-view {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  background-color: #00ffff;
  margin-top: 10rpx;
}
.content .order-view .scroll-view {
  width: 750rpx;
  background-color: #e3e3e3;
}
.content .order-view .scroll-view .scroll-body {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
}
.content .order-view .scroll-view .scroll-body .item-title-view {
  width: 750rpx;
  display: flex;
